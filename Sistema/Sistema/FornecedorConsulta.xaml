<Window x:Class="Sistema.FornecedorConsulta"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Fornecedor" Height="406" Width="596" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
<Grid Background="#FFE2E2E8">
        <TextBox Height="23" HorizontalAlignment="Stretch" Margin="14,27,5,0" Name="txtNome" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="300,76,0,0" Name="txtCnpj" VerticalAlignment="Top" Width="143" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,76,0,0" Name="txtNomeFantasia" VerticalAlignment="Top" Width="283" />
        <TextBox Height="23" HorizontalAlignment="Stretch" Margin="448,76,5,0" Name="txtIE" VerticalAlignment="Top" />
        <ComboBox Height="23" HorizontalAlignment="Stretch" Margin="484,173,5,0" Name="cbAtivo" VerticalAlignment="Top" IsEnabled="False">
            <ComboBoxItem Content="SIM" />
            <ComboBoxItem Content="NÃO" />
        </ComboBox>
        <DatePicker Height="23" HorizontalAlignment="Left" Margin="329,173,0,0" Name="dpDtCadastro" VerticalAlignment="Top" Width="149" />
        <TabControl Height="124" HorizontalAlignment="Stretch" Margin="7,204,0,0" Name="tbcEndereco" VerticalAlignment="Top" Width="562" >
            <TabItem Header="Endereço" Name="tbiEndereco">
                <Grid Name="gridEndereco">
                    <TextBox Height="23" HorizontalAlignment="Left" Margin="6,21,0,0" Name="txtCep" VerticalAlignment="Top" Width="91" />
                    <TextBox Height="23" Margin="103,21,211,0" Name="txtEndereco" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Margin="347,21,0,0" Name="txtNumero" VerticalAlignment="Top" Width="63" />
                    <TextBox Height="23" HorizontalAlignment="Left" Margin="416,21,0,0" Name="txtComplemento" VerticalAlignment="Top" Width="105" />
                    <TextBox Height="23" HorizontalAlignment="Left" Margin="241,62,0,0" Name="txtBairro" VerticalAlignment="Top" Width="189" />
                    <ComboBox Height="23" HorizontalAlignment="Left" Margin="436,62,0,0" Name="cbUF" VerticalAlignment="Top" Width="84">
                        <ComboBoxItem Content="AC" />
                        <ComboBoxItem Content="AL" />
                        <ComboBoxItem Content="AP" />
                        <ComboBoxItem Content="AM" />
                        <ComboBoxItem Content="BA" />
                        <ComboBoxItem Content="CE" />
                        <ComboBoxItem Content="DF" />
                        <ComboBoxItem Content="ES" />
                        <ComboBoxItem Content="ES" />
                        <ComboBoxItem Content="GO" />
                        <ComboBoxItem Content="MA" />
                        <ComboBoxItem Content="MT" />
                        <ComboBoxItem Content="MS" />
                        <ComboBoxItem Content="MG" />
                        <ComboBoxItem Content="PA" />
                        <ComboBoxItem Content="PB" />
                        <ComboBoxItem Content="PR" />
                        <ComboBoxItem Content="PE" />
                        <ComboBoxItem Content="PI" />
                        <ComboBoxItem Content="RJ" />
                        <ComboBoxItem Content="RN" />
                        <ComboBoxItem Content="RS" />
                        <ComboBoxItem Content="RO" />
                        <ComboBoxItem Content="RR" />
                        <ComboBoxItem Content="SC" />
                        <ComboBoxItem Content="SP" />
                        <ComboBoxItem Content="SP" />
                        <ComboBoxItem Content="SE" />
                        <ComboBoxItem Content="TO" />
                    </ComboBox>
                    <Label Content="Numero" Height="28" HorizontalAlignment="Left" Margin="343,1,0,0" Name="label9" VerticalAlignment="Top" />
                    <Label Content="Complemento" Height="28" HorizontalAlignment="Left" Margin="412,1,0,0" Name="label10" VerticalAlignment="Top" />
                    <Label Content="UF" Height="28" HorizontalAlignment="Left" Margin="432,42,0,0" Name="label11" VerticalAlignment="Top" />
                    <Label Content="Endereço" Height="28" HorizontalAlignment="Left" Margin="99,1,0,0" Name="label12" VerticalAlignment="Top" />
                    <Label Content="Bairro" Height="28" HorizontalAlignment="Left" Margin="236,42,0,0" Name="label13" VerticalAlignment="Top" />
                    <Label Content="CEP" Height="28" HorizontalAlignment="Left" Margin="2,0,0,0" Name="label14" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Margin="6,62,0,0" Name="txtCidade" VerticalAlignment="Top" Width="229" />
                    <Label Content="Cidade" Height="28" HorizontalAlignment="Left" Margin="2,42,0,0" Name="label15" VerticalAlignment="Top" />
                </Grid>
            </TabItem>
        </TabControl>
        <Label Content="Nome/Razão social" Height="28" HorizontalAlignment="Left" Name="label1" VerticalAlignment="Top" Margin="10,6,0,0" />
        <Label Content="Nome Fantasia" Height="28" HorizontalAlignment="Left" Margin="9,56,0,0" Name="label2" VerticalAlignment="Top" />
        <Label Content="CNPJ" Height="28" HorizontalAlignment="Left" Margin="296,56,0,0" Name="label4" VerticalAlignment="Top" />
        <Label Content="Ativo" Height="28" HorizontalAlignment="Left" Margin="483,153,0,0" Name="label5" VerticalAlignment="Top" />
        <Label Content="Data cadastro" Height="28" HorizontalAlignment="Left" Margin="323,153,0,0" Name="label6" VerticalAlignment="Top" />
        <Label Content="Inscrição Estadual" Height="28" HorizontalAlignment="Left" Margin="445,56,0,0" Name="label7" VerticalAlignment="Top" />
        <Button Content="Cancelar" Height="23" HorizontalAlignment="Left" Margin="495,335,0,0" Name="btCancelar" VerticalAlignment="Top" Width="75" Click="btCancelar_Click" />
        <Button Content="Cadastrar" Height="23" HorizontalAlignment="Left" Margin="335,335,0,0" Name="btCadastrar" VerticalAlignment="Top" Width="75" Click="btCadastrar_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,122,0,0" Name="txtEmail" VerticalAlignment="Top" Width="283" />
        <Label Content="Email" Height="28" HorizontalAlignment="Left" Margin="9,102,0,0" Name="label3" VerticalAlignment="Top" />
        <TextBox Height="23" Margin="299,122,5,0" Name="txtSite" VerticalAlignment="Top" HorizontalAlignment="Stretch" />
        <Label Content="Site" Height="28" HorizontalAlignment="Left" Margin="296,102,0,0" Name="label16" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="14,173,0,0" Name="txtTelefone" VerticalAlignment="Top" Width="147" />
        <Label Content="Telefone" Height="28" HorizontalAlignment="Left" Margin="9,153,0,0" Name="label8" VerticalAlignment="Top" />
        <Button Content="Editar" Height="23" HorizontalAlignment="Left" Margin="416,335,0,0" Name="btEditar" VerticalAlignment="Top" Width="75" Click="btEditar_Click" IsEnabled="False" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="167,173,0,0" Name="txtContato" VerticalAlignment="Top" Width="150" />
        <Label Content="Contato" Height="28" HorizontalAlignment="Left" Margin="163,153,0,0" Name="label17" VerticalAlignment="Top" />
    </Grid></Window>
