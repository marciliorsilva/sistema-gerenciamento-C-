        <TextBox Height="23" HorizontalAlignment="Left" Margin="180,120,0,0" Name="txtModelo" VerticalAlignment="Top" Width="150" IsEnabled="False" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="336,120,0,0" Name="txtMarca" VerticalAlignment="Top" Width="151" IsEnabled="False" />
        <Label Content="Cliente" Height="28" HorizontalAlignment="Left" Margin="258,9,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Equipamento" Height="28" HorizontalAlignment="Left" Margin="9,52,0,0" Name="label6" VerticalAlignment="Top" />
        <Label Content="Codigo da OS" Height="28" HorizontalAlignment="Left" Margin="8,9,0,0" Name="label10" VerticalAlignment="Top" />
        <Label Content="Data de Entrada" Height="28" HorizontalAlignment="Left" Margin="132,9,0,0" Name="label11" VerticalAlignment="Top" />
