        <Label Content="Modelo" Height="28" HorizontalAlignment="Left" Margin="176,100,0,0" Name="label3" VerticalAlignment="Top" />
        <Label Content="Marca" Height="28" HorizontalAlignment="Left" Margin="332,100,0,0" Name="label4" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,169,0,0" Name="txtDefeito" VerticalAlignment="Top" Width="475" IsEnabled="False" />
        <Label Content="Solicitação / Defeito" Height="28" HorizontalAlignment="Left" Margin="9,147,0,0" Name="label5" VerticalAlignment="Top" />
        <Button Content="Cadastrar" Height="23" HorizontalAlignment="Left" Margin="251,239,0,0" Name="button1" VerticalAlignment="Top" Width="75" />
        <Button Content="Editar" Height="23" HorizontalAlignment="Left" Margin="332,239,0,0" Name="button2" VerticalAlignment="Top" Width="75" IsEnabled="False" />
        <Button Content="Cancelar" Height="23" HorizontalAlignment="Left" Margin="413,239,0,0" Name="button3" VerticalAlignment="Top" Width="75" />
        <Button Content="Imprimir " Height="23" HorizontalAlignment="Left" Margin="12,239,0,0" Name="button4" VerticalAlignment="Top" Width="75" IsEnabled="False" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,210,0,0" Name="txtObs" VerticalAlignment="Top" Width="475" IsEnabled="False" />
        <Label Content="Observação" Height="28" HorizontalAlignment="Left" Margin="8,189,0,0" Name="label7" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="368,4,0,0" Name="txtCodigoCli" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
